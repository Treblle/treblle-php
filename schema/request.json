{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "api_key": {
      "type": "string"
    },
    "project_id": {
      "type": "string"
    },
    "version": {
      "type": "number"
    },
    "sdk": {
      "type": "string"
    },
    "data": {
      "type": "object",
      "properties": {
        "server": {
          "type": "object",
          "properties": {
            "ip": {
              "type": "string"
            },
            "timezone": {
              "type": "string"
            },
            "software": {
              "type": "string"
            },
            "signature": {
              "type": "string"
            },
            "protocol": {
              "type": "string"
            },
            "os": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "release": {
                  "type": "string"
                },
                "architecture": {
                  "type": "string"
                }
              }
            }
          }
        },
        "language": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "version": {
              "type": "string"
            }
          },
          "required": [
            "name",
            "version"
          ]
        },
        "request": {
          "type": "object",
          "properties": {
            "timestamp": {
              "type": "string"
            },
            "ip": {
              "type": "string"
            },
            "url": {
              "type": "string"
            },
            "user_agent": {
              "type": "string"
            },
            "method": {
              "type": "string"
            },
            "headers": {
              "type": "object"
            },
            "body": {
              "type": "object"
            },
            "raw": {
              "type": "object"
            },
            "route_path": {
              "type": "string"
            },
            "query": {
              "type": "object"
            }
          },
          "required": [
            "timestamp",
            "ip",
            "url",
            "user_agent",
            "method",
            "headers",
            "body",
            "raw"
          ]
        },
        "response": {
          "type": "object",
          "properties": {
            "headers": {
              "type": "object"
            },
            "code": {
              "type": "integer"
            },
            "size": {
              "type": "number"
            },
            "load_time": {
              "type": "number"
            },
            "body": {
              "type": "object"
            }
          },
          "required": [
            "headers",
            "code",
            "size",
            "load_time",
            "body"
          ]
        },
        "errors": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "source": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                },
                "message": {
                  "type": "string"
                },
                "file": {
                  "type": "string"
                },
                "line": {
                  "type": "integer"
                }
              },
              "required": [
                "source",
                "type",
                "message",
                "file",
                "line"
              ]
            }
          ]
        }
      },
      "required": [
        "server",
        "language",
        "request",
        "response",
        "errors"
      ]
    }
  },
  "required": [
    "api_key",
    "project_id",
    "version",
    "sdk",
    "data"
  ]
}
