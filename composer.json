{
  "type": "library",
  "name": "treblle/treblle-php",
  "description": "Stay in tune with your APIs",
  "homepage": "https://treblle.com/",
  "keywords": [
    "phpunit",
    "treblle",
    "api",
    "monitoring",
    "debuging",
    "documentation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Vedran CindriÄ‡",
      "email": "vedran@treblle.com",
      "homepage": "https://treblle.com/",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Treblle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Treblle\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.4.5|^8.0"
  },
  "require-dev": {
    "laravel/pint": "^1.15"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "pint": [
      "./vendor/bin/pint"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
